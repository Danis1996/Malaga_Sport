<!-- Profil Club -->
<div class="tm-top-c-box tm-full-width  home-about">
            <div class="uk-container uk-container-center">
                <section id="tm-top-c" class="tm-top-c uk-grid uk-grid-collapse" data-uk-grid-match="{target:'> div > .uk-panel'}" data-uk-grid-margin="">

                    <div class="uk-width-1-1 uk-width-large-1-2">
                        <div class="uk-panel">
                            <div class="va-about-wrap clearfix uk-cover-background uk-position-relative">
                                <div class="va-about-text">
                                    <div class="title">A propos : <span><%= @club.name%></span>
                                    </div>
                                    <i><em><%= @club.created_at%></em></i>
                                    <h3>Catégorie :</h3>
                                    <p><%= @club.category%></p>
                                    <h3>Les membres du club :</h3>
                                    <%@club.members.each do |member|%>
                                    <p><%= member.pseudonym%></p>
                                    <%end%>
                                    <h3>Événement créé :</h3>
                                    <% @club.events.each do |event|%>
                                    <p><%=event.title%></p>
                                    <%end%>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="uk-width-1-1 uk-width-large-1-2">
                        <div style="min-height: 497px;" class="uk-panel">
                            <div class="trainers-module tm-trainers-slider ">
                                <div class="trainer-wrapper">
                                    <div data-uk-slideset="{default: 1, animation: 'fade', duration: 400}">
                                        <div class="trainer-top">
                                            
                                            <h3>Admin Club</h3>
                                        </div>
                                        <ul class="uk-grid uk-slideset uk-grid-width-1-1">
                                            <li class="uk-active" style="">
                                                <% if @club.admin.avatar.attached?%>
                                                <%= image_tag @club.admin.avatar, alt: 'picture', :class =>"img-wrap"%>
                                                <% else %>
                                                <div class="img-wrap"><img src="/vendor/tete.gif" alt="">
                                                </div>
                                                <%end%>
                                                <div class="name"><%@club.admin.first_name%> <span><%@club.admin.last_name%></span>
                                                </div>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>

        <!-- Liste request -->
 
        <%@club.recrutements.each do |recrutement|%>

        <div class="tm-top-d-box  ">
            <div class="uk-container uk-container-center">
                <section id="tm-top-d" class="tm-top-d uk-grid" data-uk-grid-match="{target:'> div > .uk-panel'}" data-uk-grid-margin="">

                    <div class="uk-width-1-1">
                        <div class="uk-panel">
                            <div class="donate-wrap">
                                <div class="donate-inner">
                                    <h3><span>RECRUTEMENT</span></h3>
                                    <div class="uk-grid">
                                        <div class="uk-width-8-10 uk-push-1-10 intro-text">
                                            <%= recrutement.content%>   </div>
                                    </div>
                                    <form class="teamdonate-form" method="post" target="paypal">
                                        
                                        <br>
                                        <br>
                                        <a class="donate-btn" href="/requests/new"><span>Postuler</span>
                                        </a>

                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
<%end%>